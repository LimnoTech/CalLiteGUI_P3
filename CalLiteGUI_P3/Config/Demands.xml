 <panel id="demands" name="demands" visible="true" layout="GridBagLayout">
	<panel font="Tahoma-Bold-12" layout="GridBagLayout">
		<panel id="dem_SWP" resizeable="false"
			border="TitledBorder(SWP Demands - South of Delta)" preferredsize="570,280"
			layout="GridBagLayout">
			<gridbagconstraints insets="0,0,5,5"
				anchor="GridBagConstraints.WEST" gridx="0" gridy="0" />
			<buttongroup>
				<radiobutton id="dem_rdbCurSWP" name="dem_rdbCurSWP"
					text="Variable: 3.3-4.2 million acre-feet per year" minimumsize="350,20"
					selected="true">
					<gridbagconstraints insets="0,0,5,5"
						anchor="GridBagConstraints.WEST" gridx="0" gridy="0" />
				</radiobutton>
				<radiobutton id="dem_rdbFutSWP" name="dem_rdbFutSWP"
					text="Fixed (Full Table A): 4.2 million acre-feet per year"
					minimumsize="340,20">
					<gridbagconstraints insets="0,0,5,5"
						anchor="GridBagConstraints.WEST" gridx="0" gridy="1" />
				</radiobutton>
				<radiobutton name="dem_rdbUD1" id="dem_rdbUD1" text="User-Defined"
					minimumsize="340,20">
					<gridbagconstraints insets="0,0,5,5"
						anchor="GridBagConstraints.WEST" gridx="0" gridy="2" />
				</radiobutton>
			</buttongroup>
			<panel id="dem_UDSWP" resizeable="false" layout="GridBagLayout"
				preferredsize="500,280">
				<gridbagconstraints insets="0,20,5,5"
					anchor="GridBagConstraints.WEST" gridx="0" gridy="3" />
				<label id="demtxt1_t"
					text="MWDSC (cannot exceed Table A value of 1911.5 TAF/yr)">
					<gridbagconstraints insets="0,5,5,20"
						anchor="GridBagConstraints.WEST" gridx="0" gridy="0" />
				</label>
				<numtextfield id="demtxt1" name="demtxt1" enabled="false"
					MinVal="0" MaxVal="1911.5" columns="6" text="1911.5"
					HorizontalAlignment="RIGHT">
					<gridbagconstraints insets="0,0,5,5"
						fill="GridBagConstraints.HORIZONTAL" anchor="GridBagConstraints.WEST"
						gridx="1" gridy="0" gridwidth="2" />
				</numtextfield>
				<label id="demtxt2_t"
					text="Other MI (cannot exceed Table A value of 1189.4 TAF/yr)">
					<gridbagconstraints insets="0,5,5,20"
						anchor="GridBagConstraints.WEST" gridx="0" gridy="1" />
				</label>
				<numtextfield id="demtxt2" name="demtxt2" enabled="false"
					columns="6" MinVal="0" MaxVal="1189.4" text="1189.4"
					HorizontalAlignment="RIGHT">
					<gridbagconstraints insets="0,0,5,5"
						fill="GridBagConstraints.HORIZONTAL" anchor="GridBagConstraints.WEST"
						gridx="1" gridy="1" gridwidth="2" />
				</numtextfield>
				<label id="demtxt3_t" text="AG (cannot exceed Table A value of 1032.1 TAF/yr)">
					<gridbagconstraints insets="0,5,5,20"
						anchor="GridBagConstraints.WEST" gridx="0" gridy="2" />
				</label>
				<numtextfield id="demtxt3" name="demtxt3" MinVal="0"
					MaxVal="1032.1" text="1032.1" enabled="false" columns="6"
					HorizontalAlignment="RIGHT">
					<gridbagconstraints insets="0,0,5,5"
						fill="GridBagConstraints.HORIZONTAL" anchor="GridBagConstraints.WEST"
						gridx="1" gridy="2" gridwidth="2" />
				</numtextfield>
				<label id="demtxt3a_t"
					text=" Proportion of Full Article 21 (Interruptible) Demands to Deliver">
					<gridbagconstraints insets="0,5,5,20"
						anchor="GridBagConstraints.WEST" gridx="0" gridy="3" />
				</label>
				<numtextfield id="demtxt3a" name="demtxt3a" MinVal="0"
					MaxVal="1" minimumsize="100,25" text="0" enabled="false" columns="6"
					HorizontalAlignment="RIGHT">
					<gridbagconstraints insets="0,0,5,5"
						fill="GridBagConstraints.HORIZONTAL" anchor="GridBagConstraints.WEST"
						gridx="1" gridy="3" gridwidth="2" />
				</numtextfield>

				<label text="- Values for Losses are held constant " font="Tahoma-Italic">
					<gridbagconstraints insets="10,5,5,5"
						anchor="GridBagConstraints.WEST" gridx="0" gridy="4" gridwidth="2" />
				</label>
				<label text="- Article 56 demands not included " font="Tahoma-Italic">
					<gridbagconstraints insets="0,5,5,5"
						anchor="GridBagConstraints.WEST" gridx="0" gridy="5" gridwidth="2" />
				</label>
				<label text="- TAF/yr = thousands of acre-feet per year "
					font="Tahoma-Italic">
					<gridbagconstraints insets="0,5,5,5"
						anchor="GridBagConstraints.WEST" gridx="0" gridy="6" gridwidth="2" />
				</label>
			</panel>
		</panel>
		<panel id="dem_CVP" resizeable="false"
			border="TitledBorder(CVP Demands - South of Delta)" preferredsize="550,220"
			layout="GridBagLayout">
			<gridbagconstraints insets="0,0,5,5"
				anchor="GridBagConstraints.WEST" gridx="0" gridy="1" />
			<buttongroup>
				<radiobutton id="dem_rdbFullCVP" name="dem_rdbFullCVP"
					text="Full Contract" minimumsize="340,20" selected="true">
					<gridbagconstraints insets="0,0,5,5"
						anchor="GridBagConstraints.WEST" gridx="0" gridy="0" />
				</radiobutton>
				<radiobutton id="dem_rdbUD2" name="dem_rdbUD2" text="User-Defined"
					minimumsize="340,20">
					<gridbagconstraints insets="0,0,5,5"
						anchor="GridBagConstraints.WEST" gridx="0" gridy="2" />
				</radiobutton>
			</buttongroup>
			<panel id="dem_UDCVP" resizeable="false" layout="GridBagLayout">
				<gridbagconstraints insets="0,20,5,5"
					anchor="GridBagConstraints.WEST" gridx="0" gridy="3" />
				<label id="demtxt4_t"
					text="AG (cannot exceed Full Contract value of 1963.1 TAF/yr)">
					<gridbagconstraints insets="0,5,5,5"
						anchor="GridBagConstraints.WEST" gridx="0" gridy="0" />
				</label>
				<numtextfield id="demtxt4" name="demtxt4" MinVal="0"
					MaxVal="1963.1" text="1963.1" enabled="false" columns="6"
					HorizontalAlignment="RIGHT">
					<gridbagconstraints insets="0,0,5,5"
						fill="GridBagConstraints.HORIZONTAL" anchor="GridBagConstraints.WEST"
						gridx="1" gridy="0" />
				</numtextfield>
				<label id="demtxt5_t"
					text="MI (cannot exceed Full Contract value of 147.65 TAF/yr)">
					<gridbagconstraints insets="0,5,5,5"
						anchor="GridBagConstraints.WEST" gridx="0" gridy="1" />
				</label>
				<numtextfield id="demtxt5" name="demtxt5" MinVal="0"
					MaxVal="147.65" text="147.65" enabled="false" columns="6"
					HorizontalAlignment="RIGHT">
					<gridbagconstraints insets="0,0,5,5"
						fill="GridBagConstraints.HORIZONTAL" anchor="GridBagConstraints.WEST"
						gridx="1" gridy="1" />
				</numtextfield>
				<label id="demtxt6_t"
					text="Refuge (cannot exceed Full Contract value of 281.06 TAF/yr)">
					<gridbagconstraints insets="0,5,5,5"
						anchor="GridBagConstraints.WEST" gridx="0" gridy="2" />
				</label>
				<numtextfield id="demtxt6" name="demtxt6" MinVal="0"
					MaxVal="281.06 " text="281.06" enabled="false" columns="6"
					HorizontalAlignment="RIGHT">
					<gridbagconstraints insets="0,0,5,5"
						fill="GridBagConstraints.HORIZONTAL" anchor="GridBagConstraints.WEST"
						gridx="1" gridy="2" />
				</numtextfield>
				<label
					text="- Values for Water Rights, Exchanges and Losses are held constant "
					font="Tahoma-Italic">
					<gridbagconstraints insets="10,5,5,5"
						anchor="GridBagConstraints.WEST" gridx="0" gridy="3" gridwidth="2" />
				</label>
				<label text="- TAF/yr = thousands of acre-feet per year "
					font="Tahoma-Italic">
					<gridbagconstraints insets="0,5,5,5"
						anchor="GridBagConstraints.WEST" gridx="0" gridy="5" gridwidth="2" />
				</label>
			</panel>
		</panel>
		<button text="Help" id="DemandHelp" ActionCommand="AC_Help">
			<gridbagconstraints anchor="GridBagConstraints.WEST"
				gridx="0" gridy="99" gridwidth="2" insets="5,5,5,0" />
		</button>
	</panel>
	<panel preferredsize="400,370">
		<gridbagconstraints insets="0,25,20,0"
			anchor="GridBagConstraints.NORTH" gridx="2" gridy="0" />
		<label id="map2" icon="images/aqueduct_demand.gif" />
	</panel>
</panel>