<panel id="externalPDF" name="externalPDF" font="Tahoma-Italic-12" 
	layout="GridBagLayout">
	<label text="Report template file">
		<gridbagconstraints insets="0,5,5,5"
			anchor="GridBagConstraints.WEST" gridx="0" gridy="5" />
	</label>
	<textfield text="callite_scenario_comparison.inp"
		tooltiptext="Config\\callite_scenario_comparison.inp" enabled="false"
		id="tfTemplateFILE" preferredsize="250,20" name="tfTemplateFILE">
		<gridbagconstraints insets="0,5,5,5"
			anchor="GridBagConstraints.WEST" gridx="1" gridy="5" />
	</textfield>
	<button text="Select File ..." id="btnGetTemplateFile" name="btnGetTemplateFile">
		<gridbagconstraints insets="0,5,5,5"
			anchor="GridBagConstraints.WEST" gridx="2" gridy="5" />
	</button>
	<label text="DSS Result File #1">
		<gridbagconstraints insets="0,5,5,5"
			anchor="GridBagConstraints.WEST" gridx="0" gridy="10" />
	</label>
	<textfield text="" enabled="false" id="tfReportFILE1"
		preferredsize="250,20" name="tfReportFILE1">
		<gridbagconstraints insets="0,5,5,5"
			anchor="GridBagConstraints.WEST" gridx="1" gridy="10" />
	</textfield>
	<button text="Select File ..." id="btnGetReportFile1" name="btnGetReportFile1">
		<gridbagconstraints insets="0,5,5,5"
			anchor="GridBagConstraints.WEST" gridx="2" gridy="10" />
	</button>
	<label text="DSS Result Alias #1">
		<gridbagconstraints insets="0,5,5,5"
			anchor="GridBagConstraints.WEST" gridx="0" gridy="20" />
	</label>
	<textfield text="CalLite 1" id="tfReportNAME1" name="tfReportNAME1"
		preferredsize="250,20">
		<gridbagconstraints insets="0,5,5,5"
			anchor="GridBagConstraints.WEST" gridx="1" gridy="20" />
	</textfield>
	<label text="DSS Result File #2">
		<gridbagconstraints insets="10,5,5,5"
			anchor="GridBagConstraints.WEST" gridx="0" gridy="30" />
	</label>
	<textfield text="" enabled="false" id="tfReportFILE2"
		preferredsize="250,20" name="tfReportFILE2">
		<gridbagconstraints insets="10,5,5,5"
			anchor="GridBagConstraints.WEST" gridx="1" gridy="30" />
	</textfield>
	<button text="Select File ..." id="btnGetReportFile2" name="btnGetReportFile2">
		<gridbagconstraints insets="10,5,5,5"
			anchor="GridBagConstraints.WEST" gridx="2" gridy="30" />
	</button>
	<label text="DSS Result Alias #2">
		<gridbagconstraints insets="0,5,5,5"
			anchor="GridBagConstraints.WEST" gridx="0" gridy="40" />
	</label>
	<textfield text="CalLite 2" id="tfReportNAME2" name="tfReportNAME2"
		preferredsize="250,20">
		<gridbagconstraints insets="0,5,5,5"
			anchor="GridBagConstraints.WEST" gridx="1" gridy="40" />
	</textfield>
	<label text="Report output file">
		<gridbagconstraints insets="10,5,5,5"
			anchor="GridBagConstraints.WEST" gridx="0" gridy="50" />
	</label>
	<textfield text="report.pdf" tooltiptext=".//Scenarios//report.pdf"
		enabled="false" id="tfReportFILE3" preferredsize="250,20" name="tfReportFILE3">
		<gridbagconstraints insets="10,5,5,5"
			anchor="GridBagConstraints.WEST" gridx="1" gridy="50" />
	</textfield>
	<button text="Select File ..." id="btnGetReportFile3" name="btnGetReportFile3">
		<gridbagconstraints insets="10,5,5,5"
			anchor="GridBagConstraints.WEST" gridx="2" gridy="50" />
	</button>
	<label text="Notes" minimumsize="120,20" preferredsize="120,20">
		<gridbagconstraints insets="10,5,5,5"
			anchor="GridBagConstraints.NORTHWEST" gridx="0" gridy="60" gridwidth="2" />
	</label>
	<scrollpane preferredsize="400,50" minimumsize="400,50">
		<gridbagconstraints insets="0,5,5,5"
			anchor="GridBagConstraints.NORTHWEST" gridx="0" gridy="65" gridwidth="3" />
		<textarea linewrap="true" wrapstyleword="true" text=""
			id="taReportNOTES" name="taReportNOTES"></textarea>
	</scrollpane>
	<label text="Assumptions" minimumsize="120,20" preferredsize="120,20">
		<gridbagconstraints insets="10,5,5,5"
			anchor="GridBagConstraints.NORTHWEST" gridx="0" gridy="70" gridwidth="3" />
	</label>
	<scrollpane preferredsize="400,50" minimumsize="400,50">
		<gridbagconstraints insets="0,5,5,5"
			anchor="GridBagConstraints.NORTHWEST" gridx="0" gridy="75" gridwidth="3" />
		<textarea text=" " id="taReportASSUMPTIONS" name="taReportASSUMPTIONS"></textarea>
	</scrollpane>
	<label text="Modeler">
		<gridbagconstraints insets="10,5,5,5"
			anchor="GridBagConstraints.WEST" gridx="0" gridy="80" />
	</label>
	<textfield text="" id="tfReportMODELER" name="tfReportMODELER"
		preferredsize="250,20">
		<gridbagconstraints insets="10,5,5,5"
			anchor="GridBagConstraints.WEST" gridx="1" gridy="80" />
	</textfield>
	<label text="Table Font Size (points)">
		<gridbagconstraints insets="10,5,5,5"
			anchor="GridBagConstraints.WEST" gridx="0" gridy="85" />
	</label>
	<textfield id="tfFontSize" name="tfFontSize" text="9"
		preferredsize="50,20">
		<gridbagconstraints insets="10,5,5,5"
			anchor="GridBagConstraints.WEST" gridx="1" gridy="85" />
	</textfield>
	<button name="btnReport" id="btnReport" text="Generate Report"
		ActionCommand="AC_GenReport">
		<gridbagconstraints insets="10,5,5,5"
			anchor="GridBagConstraints.EAST" gridx="2" gridy="90" />
	</button>
	<button text="Help" id="ReportHelp" ActionCommand="AC_Help">
		<gridbagconstraints anchor="GridBagConstraints.WEST"
			gridx="0" gridy="95" gridwidth="2" insets="5,5,5,0" />
	</button>
</panel>